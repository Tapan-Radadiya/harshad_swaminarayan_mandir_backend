{
  "kind": "collectionType",
  "collectionName": "image_managers",
  "info": {
    "singularName": "image-manager",
    "pluralName": "image-managers",
    "displayName": "image-manager"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image_type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "carousel",
        "gallery",
        "section_banner"
      ]
    },
    "images": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "priority": {
      "type": "integer",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "required": true
    },
    "gallery_group": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::gallery-group.gallery-group"
    }
  }
}
